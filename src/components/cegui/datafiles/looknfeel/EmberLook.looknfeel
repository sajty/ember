			</Affector>
			<Affector property="Visible" interpolator="bool">
				<KeyFrame position="0.0" sourceProperty="Visible" />
				<KeyFrame position="0.4" value="false" progression="discrete" />
			</Affector>
			
			<Subscription event="StartHideTransition" action="Start" />
			
