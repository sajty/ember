        </StateImagery>
 		<AnimationDefinition name="ShowTransition" duration="0.4" replayMode="once">
			<Affector property="Alpha" interpolator="float">
				<KeyFrame position="0.0" value="0.0" />
				<KeyFrame position="0.2" value="0.8" progression="quadratic accelerating" />
			</Affector>
			
			<Subscription event="Shown" action="Start" />
			<Subscription event="Hidden" action="Pause" />
		</AnimationDefinition>
		<AnimationDefinition name="HideTransition" duration="0.4" replayMode="once">
			<Affector property="Alpha" interpolator="float">
				<KeyFrame position="0.0" sourceProperty="Alpha" />
				<KeyFrame position="0.4" value="0.0" progression="linear" />
			</Affector>

			<Affector property="Visible" interpolator="bool">
				<KeyFrame position="0.0" sourceProperty="Visible" />
				<KeyFrame position="0.4" value="false" progression="discrete" />
			</Affector>

			
			<Subscription event="StartHideTransition" action="Start" />
			
			<!-- stop the transition in case user changes his/her mind -->
			<Subscription event="Shown" action="Pause" />
		</AnimationDefinition>
    </WidgetLook>	
